lookup calt_Non_Colliding_Forms {
  lookupflag 0;
    @cc31_match_1 = [ emacron imacron ];
    @cc31_match_2 = [ eth ];
    sub @cc31_match_1 @cc31_match_2' lookup Single_Substitution_01  ;
    @cc32_match_1 = [ f uniFB00 ];
    @cc32_match_2 = [ agrave acircumflex atilde adieresis aring egrave
    		      ecircumflex edieresis igrave iacute icircumflex
    		      idieresis ograve ocircumflex otilde odieresis
    		      ugrave ucircumflex udieresis ydieresis amacron
    		      abreve emacron ebreve imacron ibreve omacron
    		      obreve umacron ubreve ];
    sub @cc32_match_1' lookup Single_Substitution_01  @cc32_match_2 ;
    @cc33_match_1 = [ k_i ];
    @cc33_match_2 = [ f g j p f_f.alt f_f_t f_f_udieresis f_f_y uniFB00 uniFB01 uniFB02 
	uniFB03 uniFB04 uniFB05 longs uniE1A6 uniEBA1 uniEBA2 uniF4FC
	uniEBA3 uniEBA6 uniEBA7 uniEBA8 uniEBA0
	uniEBA1 uniEBA2 uniEBA3 uniEBA4 uniEBA5 uniEBA6 uniEBA7 
	uniEBA8 uniEBA9 uniEBAA uniEBAB uniEBAC uniEBAD 
	uniEBAE uniEBAF uniF4FA uniF4FB uniF4FC uniF4FD uniF4FE
	uniF4FF uniEEC9 uniEECA uniEECB f_thorn f_udieresis f_y 
	f_thorn.nordic f_eth.nordic ];
    sub @cc33_match_1' @cc33_match_2  by [ k_i.alt ] ;
} calt_Non_Colliding_Forms;

lookup calt_j_After_Ogonek {
  lookupflag 0;
    @cc34_match_1 = [ g Aogonek aogonek Eogonek eogonek uogonek ];
    @cc34_match_2 = [ j ];
